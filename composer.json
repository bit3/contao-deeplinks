{
	"name": "bit3/contao-deeplinks",
	"description":"Create deeplinks for your Contao modules in the backend menu.",
	"keywords":["contao", "module", "deeplinks", "hotlinks"],
	"type": "contao-module",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Tristan Lins",
			"email":"tristan.lins@bit3.de",
			"homepage":"http://bit3.de",
			"role":"Developer"
		}
	],
	"support":{
		"email":"info@bit3.de",
		"issues":"https://github.com/bit3/contao-console/issues",
		"irc":"irc://irc.freenode.org/contao.dev",
		"source":"https://github.com/bit3/contao-console"
	},
	"require": {
		"php": ">=5.3",
		"contao/core": ">=2.11,<3.3",
		"contao-community-alliance/composer-installer": "*",
		"bit3/contao-dependency-container": "~1.3",
		"bit3/contao-event-dispatcher":"~1"
	},
	"require-dev":{
		"bit3/php-coding-standard":"2.*"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload":{
		"psr-0":{
			"Bit3\\Contao\\Deeplinks": "src/"
		}
	},
	"extra": {
		"contao": {
			"sources": {
				"bin": "bin"
			}
		},
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
